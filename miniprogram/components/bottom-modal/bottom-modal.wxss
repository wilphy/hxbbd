.modal {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 999;
  text-align: center;
  background: rgba(0, 0, 0, 0.6);
}

.panel {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  min-height: 300rpx;
  background: #f8f8f8;
  box-sizing: border-box;
}

.icon-shanchu1 {
  position: absolute;
  right: 6rpx;
  top: 6rpx;
  padding: 20rpx;
  font-size: 44rpx;
}